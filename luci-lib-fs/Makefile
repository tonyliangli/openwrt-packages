include $(INCLUDE_DI$(TOPDIR)/feeds/packagesckage.mk

define Packa$(TOPDIR)/feeds/packagesci-lib-fs
  SECTION:=lang
  CATEGORY:=Languages
  SUBMENU:=Lua
