	$(CP)$(TOPDIR)/feeds/packagesl$(TOPDIR)/feeds/packages.lua $($(TOPDIR)/feeds/packagesr/l$(TOPDIR)/feeds/packagesa/lu$(TOPDIR)/feeds/packages.lua
